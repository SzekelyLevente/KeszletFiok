<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KeszletFiok.Settings"
             Title="Beállítások">
    <ScrollView>
        <VerticalStackLayout
            Padding="30,0"
            Spacing="5">
            <Label Text="készletfiók: "/>
            <Entry x:Name="inputAccount"/>
            <Label Text="első fiók: "/>
            <Entry x:Name="inputFirst"/>
            <Button x:Name="btnMentes" Text="mentés" Clicked="btnMentes_Clicked"/>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>